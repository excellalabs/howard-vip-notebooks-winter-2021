   "source": [
    "import pandas as pd\n",
    "import re\n",
    "from collections import Counter"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "568b50af",
   "metadata": {},
   "outputs": [],
   "source": [
    "df = pd.read_csv('NOTEEVENTS.csv.gz', nrows=10000)['TEXT']\n",
    "stars = []\n",
    "for row in df:\n",
    "    for x in re.findall(r'\\[\\*\\*.+?\\*\\*\\]', row):\n",
    "        stars.append(x)\n",
    "counts = Counter(stars)\n",
    "counts.most_common(100)"
   ]
  }
